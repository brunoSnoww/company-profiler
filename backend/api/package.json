  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^10.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^4.2.0",
